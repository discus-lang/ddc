-- Compile application of anonymous function.
module Test with
letrec {
  app : [a$r b$r : Region].Ptr# rT Obj -> Ptr# a$r Obj -> Ptr# b$r Obj
    = /\(a$r b$r : Region).
       \(f : Ptr# rT Obj).\(x : Ptr# a$r Obj).
      makePtr# [b$r] [Obj]
          (apply1 (takePtr# [rT] [Obj] f) (takePtr# [a$r] [Obj] x))
}


