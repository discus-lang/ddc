module Module
exports {
    boxi    :: Int r0# -> Int r0#;
    unboxi  :: Int r0# -> Int r0#;

    boxi_   :: [r : Region]. Int r -> Int r;
    unboxi_ :: [r : Region]. Int r -> Int r;
} with letrec

boxi (x : Int r0#) : Int r0#
    = x

unboxi (x : Int r0#) : Int r0#
    = x


boxi_ [r : Region] (x : Int r) : Int r
    = x

unboxi_ [r : Region] (x : Int r) : Int r
    = x
