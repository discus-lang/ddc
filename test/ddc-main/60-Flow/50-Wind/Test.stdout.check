
-- Simple loop with no scalar results.
module Main with
letrec {
  lower_map_map_rq2 : [k_d : Rate].Series# k_d Int# -> Vector# Int#
    = /\(k_d : Rate).
       \(s_aub : Series# k_d Int#).
      let x6 : Vector# Int#
            = vnewN# [Int#] [k_d] (rateOfSeries# [k_d] [Int#] s_aub) in
      let loop$length : Nat#
            = natOfRateNat# [k_d] (rateOfSeries# [k_d] [Int#] s_aub) in
      letrec {
        loop : Nat# -> Unit
          = \(x0 : Nat#).
            case sub# [Nat#] loop$length x0 of {
              0i#  
               -> ();
              _ 
               -> let s_aub__elem : Int# = next# [Int#] [k_d] s_aub x0 in
                  let x2__elem : Int# = add# [Int#] s_aub__elem 1i# in
                  let x4__elem : Int# = mul# [Int#] x2__elem 2i# in
                  let _ : Unit = vwrite# [Int#] x6 x0 x4__elem in
                  loop (add# [Nat#] x0 1i#)
            }
      } in
      let _ : Unit = loop 0i# in
      x6
}


-- Loop with a guard and a scalar result.
module Main 
imports {
        maxx :: Int# -> Int# -> Int#;
} with
letrec {
  filterMax : [k : Rate].Series# k Int# -> Tuple2# (Vector# Int#) Int#
    = /\(k : Rate).
       \(s1 : Series# k Int#).
      let k2__count$0 : Nat# = 0# in
      let x5 : Vector# Int#
            = vnewN# [Int#] [k] (rateOfSeries# [k] [Int#] s1) in
      let x9__acc$0 : Int# = 0i# in
      let loop$length : Nat#
            = natOfRateNat# [k] (rateOfSeries# [k] [Int#] s1) in
      letrec {
        loop : Nat# -> Nat# -> Int# -> Tuple2# Nat# Int#
          = \(x2 k2__count$1 : Nat#).\(x9__acc$1 : Int#).
            case sub# [Nat#] loop$length x2 of {
              0i#  
               -> T2# [Nat#] [Int#] k2__count$1 x9__acc$1;
              _ 
               -> let s1__elem : Int# = next# [Int#] [k] s1 x2 in
                  let x0 : Int# = mod# [Int#] s1__elem 2i# in
                  let x3__elem : Bool# = eq# [Int#] x0 0i# in
                  case x3__elem of {
                    True#  
                     -> let x3 : Nat# = k2__count$1 in
                        let _ : Unit = vwrite# [Int#] x5 x3 s1__elem in
                        let x6 : Int# = x9__acc$1 in
                        let x9__acc$2 : Int# = maxx x6 s1__elem in
                        loop (add# [Nat#] x2 1i#) (add# [Nat#] k2__count$1 1i#) x9__acc$2;
                    _ 
                     -> loop (add# [Nat#] x2 1i#) k2__count$1 x9__acc$1
                  }
            }
      } in
      letcase T2# (k2__count$2 : Nat#) (x9__acc$2 : Int#)  = loop 0i# k2__count$0 x9__acc$0 in
      let x4 : Nat# = k2__count$2 in
      let x6 : Vector# Int# = vslice# [Int#] x4 x5 in
      let x9 : Int# = x9__acc$2 in
      T2# [Vector# Int#] [Int#] x6 x9
}


-- Split loop from the vector scheduler.
