             loop : Nat# -> Vec4# Float32# -> Vec4# Float32# -> Tuple2# (Vec4# Float32#) (Vec4# Float32#)
               = \(x5 : Nat#).\(x1$vec$1 x2$vec$1 : Vec4# Float32#).
                 case sub# [Nat#] loop$length x5 of {
